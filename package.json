{
  "name": "event-calendar",
  "version": "1.0.0",
  "description": "my event calendar app",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run preview --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postBuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.3.0",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "styled-reset": "^4.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  },
  "author": "Md. Obaydul Hoque",
  "license": "MIT"
}